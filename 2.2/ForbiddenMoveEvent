import java.util.EventObject;

public class ForbiddenMoveEvent extends EventObject{
	/*Evenement qui permet de gérer les limites des déplacements des robots
	 * au sein de l'onglet simulation*/
	
	float posX;
	float posY;

	public ForbiddenMoveEvent(Object source, float posX, float posY) {
		super(source);
		this.posX = posX;
		this.posY = posY;
	}

}
