import java.util.EventObject;

public class CheckCollisionEvent extends EventObject{
	/*Evenement qui permet de gérer les possibles collisions qui
	 * entre les robots. La distance minimales de proximité est fixée à 30*/
	
	float posX;
	float posY;

	public CheckCollisionEvent(Object source,float posX, float posY){
		super(source);
		this.posX = posX;
		this.posY = posY;
	}
}
